\begin_inset Formula $\alpha=\frac{1}{2}$
\end_inset

, 
\begin_inset Formula $\beta=-\frac{1}{8}$
\end_inset

)
\end_layout

\end_inset


\end_layout

\begin_layout Plain Layout

